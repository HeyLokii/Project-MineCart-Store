        }
        vf = !0;
        {
            console.warn("⚠️ Firebase auth não disponível - usando modo offline"),
            i(!1);
            return
        }
    }
    , [p]);
    const x = async () => {
        throw new Error("Firebase não configurado - login indisponível")
    }
      , j = f.useCallback(async () => {
        {
